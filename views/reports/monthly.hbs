<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-black">
            <div class="panel-body">
                <form method="GET" action="mreport">
                <select name="currency" id="currency">
                    {{#currencies}}
                        <option value="{{this}}">{{this}}</option>
                    {{/currencies}}
                </select>
                <input name="month" id= "month" type=month value="{{dateFormat date "YYYY-MM" true}}" />
                <button type="submit" >Generate</button>
                </form>
            </div>
        </div>
    </div>
</div>
